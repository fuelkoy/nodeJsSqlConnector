{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noEmit": true,
    "strict": true,
    "lib": ["ES2022"],
    "target": "ES2022",
  },
  "compileOnSave": true,
  "include": [
    "src"
  ]
}
